<launch>

  <!--name argument is not required, because it will overwrite all node name in the exec-->
  <node pkg="lego_loam_bor" exec="lego_loam_bag_lidar2depthimg_node" output="screen" respawn="false">
    <param from="$(find-pkg-share lego_loam_bor)/config/loam_bag_lidar2depthimg_config.yaml" />
  </node>

  <node pkg="rviz2" exec="rviz2" output="screen" respawn="false" args="-d $(find-pkg-share lego_loam_bor)/rviz/lego_loam.rviz">
  </node>

  <!--- TF -->
  
  <node pkg="tf2_ros" exec="static_transform_publisher" name="sensor2baselink"  args="0.3 0 0.5 0.0 1.5707 0 directdrive_base_link rslidar_airy_front" />
</launch>
